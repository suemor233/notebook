{
  "name": "notebook",
  "scripts": {
    "dev": "vitepress dev src --open",
    "build": "vitepress build src",
    "serve": "vitepress serve src",
    "format": "prettier --write src/**"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md}": [
      "prettier --ignore-path ./.prettierignore --write "
    ]
  },
  "author": "suemor",
  "dependencies": {
    "vite": "3.2.3",
    "vitepress": "1.0.0-alpha.28"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "prettier": "2.7.1"
  }
}